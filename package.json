{
  "name": "hypernative-webapp-client",
  "version": "1.1.2",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.1",
    "@mui/x-date-pickers": "^5.0.0-beta.6",
    "@reduxjs/toolkit": "^1.8.3",
    "chart.js": "^3.9.1",
    "history": "^5.3.0",
    "http-status-codes": "^2.2.0",
    "i18next": "^21.8.14",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-i18next": "^11.18.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.7",
    "typescript": "^4.7.4",
    "url-join": "^5.0.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-window": "^1.8.5",
    "babel-plugin-import": "^1.13.5",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.2.1"
  },
  "scripts": {
    "postinstall": "node create-env.js",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            "@mui/*/*/*",
            "!@mui/material/test-utils/*",
            "@emotion/*"
          ]
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false,
    "arrowParens": "avoid"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
